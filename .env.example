# Google Ads API Configuration
# Copy this file to .env and fill in your actual credentials

# Developer token from Google Ads API Center
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here

# OAuth2 Client ID from Google Cloud Console
GOOGLE_ADS_CLIENT_ID=your_client_id_here.apps.googleusercontent.com

# OAuth2 Client Secret from Google Cloud Console
GOOGLE_ADS_CLIENT_SECRET=your_client_secret_here

# OAuth2 Refresh Token (generated using google-ads-generate-refresh-token)
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token_here

# Google Ads Customer ID (without hyphens, e.g., 1234567890)
GOOGLE_ADS_CUSTOMER_ID=your_customer_id_here

# Optional: Login Customer ID (for manager accounts)
GOOGLE_ADS_LOGIN_CUSTOMER_ID=
